{
  "expo": {
    "name": "pbl6-mobile",
    "slug": "pbl6-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/image/big-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/image/big-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/image/big-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/image/big-logo.png"
    },
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
